{
  "name": "safir-from-npm-examples",
  "version": "1.0.0",
  "description": "Ultimate perfomance Ultimate simple!",
  "main": "index.js",
  "scripts": {
    "demo1": "build-assets.sh | watchify hello/demo1.js -p [esmify --noImplicitAny] -o dist/bundle.js",
    "demo2": "build-assets.sh | watchify hello/demo2.js -p [esmify --noImplicitAny] -o dist/bundle.js",
    "demo3": "build-assets.sh | watchify hello/demo3.js -p [esmify --noImplicitAny] -o dist/bundle.js",
    "demo4": "build-assets.sh | watchify hello/demo4.js -p [esmify --noImplicitAny] -o dist/bundle.js",
    "demo5": "build-assets.sh | watchify hello/demo5.js -p [esmify --noImplicitAny] -o dist/bundle.js",
    "all.demos": "build-assets.sh | browserify hello/all-demos.js -p esmify > dist/bundle.js",
    "build.demo1": "build-assets.sh | browserify hello/demo1.js -p esmify > dist/demo1.js",
    "build.demo2": "build-assets.sh | browserify hello/demo2.js -p esmify > dist/demo2.js",
    "build.demo3": "build-assets.sh | browserify hello/demo3.js -p esmify > dist/demo3.js",
    "build.demo4": "build-assets.sh | browserify hello/demo4.js -p esmify > dist/demo4.js",
    "build.demo5": "build-assets.sh | browserify hello/demo5.js -p esmify > dist/demo5.js",
    "build.all": "build-assets.sh | npm run build.demo1 | npm run build.demo2 | npm run build.demo3 | npm run build.demo4 | npm run build.demo5",
    "host": "http-server dist/ -p 80",
    "https": "http-server ./dist/ -S -C cert.pem -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "browser-resolve": "^2.0.0",
    "watchify": "^4.0.0",
    "safir": "^1.1.10"
  },
  "devDependencies": {
    "esmify": "^2.1.1"
  },
  "author": "",
  "license": "MIT"
}
